{
  "name": "EmoryGPT",
  "version": "1.0",
  "manifest_version": 3,
  "description": "Chrome extension for EmoryGPT",
  "content_scripts": [
    {
      "matches": ["https://chat.openai.com/*"],
      "css": ["app/style.css"],
      "js": [
        "app/data.js",
        "app/formulas/salespage.js",
        "app/formulas/optin.js",
        "app/actions/showPopup.js",
        "app/actions/runPrompt.js",
        "app/index.js"
      ]
    }
  ]
}
